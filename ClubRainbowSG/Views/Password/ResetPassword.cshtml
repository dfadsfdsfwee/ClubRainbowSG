

@{
    ViewData["Title"] = "ResetPassword";
}

<title>ResetPassword</title>
<form action="ResetPassword" method="post" class="form-group">

    <div class="form-div-groups">
        <h1>Reset Password</h1>
        <h2>Please enter your email, we will send verification code to your email.</h2>
    </div>
    <div class="form-div-groups">
        <label for="email">Email</label><br>
        <input type="text" id="email" name="email" class="form-container" placeholder="Your Email" />
    </div>
    <button type="submit" class="btn">Send</button>
</form>


