@model ClubRainbowSG.Models.TestProgramme
@{
    ViewBag.Title = "Event Home";
}

@{
    var pcsNames = ViewBag.PCSNames as List<string>;
}

<h2>Select PCS Name</h2>

@if (pcsNames != null && pcsNames.Any())
{
    <form method="post" action="/Home/SelectPCSName">
        <!-- Adjust action URL as needed -->
        <select name="selectedPCSName" id="selectedPCSName">
            @foreach (var pcsName in pcsNames)
            {
                <option value="@pcsName">@pcsName</option>
            }
        </select>
       
    </form>
}
else
{
    <p>No PCS names available.</p>
}



